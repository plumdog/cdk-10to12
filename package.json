{
    "name": "cdk-10to12",
    "version": "0.1.0",
    "description": "Hack all node10 Lambdas to use node12",
    "license": "MIT",
    "author": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/plumdog/cdk-10to12"
    },
    "keywords": [
        "cdk",
        "lambda",
        "node"
    ],
    "scripts": {
        "test": "jest",
        "tsc": "tsc",
        "clean": "rm -rf build/",
        "compile": "npm run clean && npm run -- tsc --outDir build/ && cp package.json README.md ./build/",
        "version": "auto-changelog --template ./changelog_template.hbs -p && git add CHANGELOG.md",
        "lint": "eslint 'index.ts' 'test/**/*.ts' jest.config.js --fix",
        "lint-check": "eslint 'index.ts' 'test/**/*.ts' jest.config.js --max-warnings 0",
        "test-old-dependencies": "rm -rf _test_old_dependencies/ && mkdir _test_old_dependencies/ && cp -R index.ts test/ jest.config.js tsconfig.json ./_test_old_dependencies/ && cp package_test_old_deps.json ./_test_old_dependencies/package.json && (cd _test_old_dependencies/ && npm i && npm run test)"
    },
    "peerDependencies": {
        "@aws-cdk/aws-lambda": "^1.18.0",
        "@aws-cdk/core": "^1.18.0"
    },
    "devDependencies": {
        "@types/node": ">=10",
        "typescript": ">=3.0.0",
        "@aws-cdk/assert": "^1.18.0",
        "auto-changelog": "^1.16.2",
        "@aws-cdk/aws-lambda": "^1.18.0",
        "@aws-cdk/core": "^1.18.0",
        "jest": "^26.0.0",
        "ts-jest": "^26.0.0",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.9.0",
        "eslint-plugin-prettier": "3.1.2",
        "prettier": "1.19.1",
        "@types/jest": "^26.0.0",
        "@typescript-eslint/eslint-plugin": "4.8.2",
        "@typescript-eslint/parser": "4.8.2"
    }
}
